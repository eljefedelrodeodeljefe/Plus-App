{
  "name": "ionic-project",
  "version": "1.1.1",
  "description": "An Ionic project",
  "scripts": {
    "emu-ios": "ionic emulate ios --target='iPhone-6s, 9.3' --livereload",
    "test": "karma start karma.conf.js",
    "dev": "watchify --detect-globals --ignore-missing -vd -e src/main.js -o www/dist/build.js",
    "dev-app": "watchify --detect-globals --ignore-missing -vd -e src/main.js -o dist/build.js & electron index.js",
    "build": "cross-env NODE_ENV=production browserify -e src/main.js | uglifyjs -c warnings=false -m > www/dist/build.js",
    "lint": "eslint --ext .js,.vue src test/unit",
    "publish": "electron-packager . AppName-Vue-Boilerplate --all --overwrite --out=dist/publish/ --ignore=node_modules/ --ignore=node_modules/dist/publish/",
    "pack": "node dist.js"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "dependencies": {
    "gulp": "^3.5.6",
    "gulp-concat": "^2.2.0",
    "gulp-minify-css": "^0.3.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^2.0.4",
    "marked": "^0.3.5"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.6.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.5",
    "diff2html": "^2.0.0-beta16",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^1.0.0",
    "eslint": "^2.9.0",
    "eslint-plugin-html": "^1.1.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-browserify": "^5.0.5",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "level-js": "^2.2.3",
    "levelup": "^1.3.1",
    "phantomjs-prebuilt": "^2.1.3",
    "proxyquireify": "^3.0.1",
    "superstatic": "^4.0.2",
    "uglify-js": "^2.5.0",
    "vue-hot-reload-api": "^1.2.2",
    "vue-resource": "^0.7.0",
    "vue-router": "^0.7.11",
    "vue-touch": "^1.0.2",
    "vueify": "^8.0.0",
    "vueify-insert-css": "^1.0.0",
    "watchify": "^3.4.0"
  },
  "optionalDependencies": {
    "appdmg": "^0.3.7"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "android"
  ]
}
